{
	"name": "PL_ConsumirRestComoDataSet_CSV",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "RestSource",
						"httpRequestTimeout": "00:01:40",
						"requestInterval": "00.00:00:00.010",
						"requestMethod": "GET"
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['$id']"
								},
								"sink": {
									"name": "id",
									"type": "Int64"
								}
							},
							{
								"source": {
									"path": "$['currentDateTime']"
								},
								"sink": {
									"name": "currentDateTime",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['utcOffset']"
								},
								"sink": {
									"name": "utcOffset",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['isDayLightSavingsTime']"
								},
								"sink": {
									"name": "isDayLightSavingsTime",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['dayOfTheWeek']"
								},
								"sink": {
									"name": "dayOfTheWeek",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['timeZoneName']"
								},
								"sink": {
									"name": "timeZoneName",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['currentFileTime']"
								},
								"sink": {
									"name": "currentFileTime",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['ordinalDate']"
								},
								"sink": {
									"name": "ordinalDate",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['serviceResponse']"
								},
								"sink": {
									"name": "serviceResponse",
									"type": "String"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "Rest_DataSet",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "CSV_ResultadoRest",
						"type": "DatasetReference"
					}
				]
			}
		],
		"folder": {
			"name": "Clase Asincronica"
		},
		"annotations": []
	}
}